<launch>
        <node pkg="joy" type="joy_node" name="joystick_bridge" ns="para">
            <param name="dev" type="str" value="/dev/input/js0"/>
            <param name="deadzone" type="double" value="0.20"/>
            <param name="autorepeat_rate" type="double" value="0.0"/>
        </node>

        <node pkg="para_control" type="joy_to_motor_command.py" name="joy_to_motor_command" ns="para" output="screen">
        </node>
</launch>
